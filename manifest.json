{
    "manifest_version": 2,
  
    "name": "Sohail Launcher",
    "description": "Quick launch for Sohail Media",
    "version": "1.0",
    "background": {
      "scripts": ["popup.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "./images/bolt_16.png",
        "24": "./images/bolt_24.png",
        "32": "./images/bolt_32.png",
        "48": "./images/bolt_48.png",
        "128": "./images/bolt_128.png"
      },
      "default_title": "View Syed Sohail Ahmed's Social Media"
    },
    "icons": {
      "16": "./images/bolt_16.png",
      "24": "./images/bolt_24.png",
      "32": "./images/bolt_32.png",
      "48": "./images/bolt_48.png",
      "128": "./images/bolt_128.png"
    },
    "permissions": ["activeTab"]
  }